<form action= "<%=users_url%>" method="post">
  <label for="user_name">Name</label>
  <input
    type="text"
    name="user[user_name]"
    id="user_user_name"
    value="<%= @user.user_name %>">
  <br>
  <label for="password">Password</label>
  <input
    type="text"
    name="user[password]"
    id="user_password"
    value="<%= @user.password %>">
  <br>
  <input type="submit" value="sign up!">

</form>
