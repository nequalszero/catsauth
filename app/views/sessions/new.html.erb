<h1>Log in</h1>
<form action= "<%=session_url%>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label for="user_name">Name</label>
  <input
    type="text"
    name="user[user_name]"
    id="user_user_name">
  <br>
  <label for="password">Password</label>
  <input
    type="text"
    name="user[password]"
    id="user_password">
  <br>
  <input type="submit" value="log in!">

</form>
